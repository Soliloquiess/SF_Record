					paper[i][j]=i;
				}
			}
		}
		
		int cnt = 0;
		for (int i = 1; i <= n; i++) {
			cnt = 0;
			for (int j = 0; j <= 100; j++) {
				for (int k = 0; k <= 100; k++) {
