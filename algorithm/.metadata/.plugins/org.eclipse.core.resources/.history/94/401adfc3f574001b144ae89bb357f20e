package BOJ;

import java.util.Scanner;

public class BOJ_10163_»öÁ¾ÀÌ {
	static int[][] paper = new int[102][102];
	public static void main(String[] args) {
		Scanner sc = new Scanner (System.in);
		int a, int b, int height, int weight;
		int T = sc.nextInt();
		for(int i = 0; i<T;i++) {
				 a = sc.nextInt();
				 b = sc.nextInt();
				 height = sc.nextInt();
				 weight = sc.nextInt();
			for(int j = a; a+height ; a++) {
				paper[i][j]=1;
			}
		}
	}
}
