package com.thread;

import java.util.Date;

public class TheradClock implements Runnable {
	
	ThreadClock(){
		Thread t  =  new Thread();
		t.start();
	}
	
	public static void main(String[] args) {
		new TheradClock();
	}
	
	//thread 작업내용. 1초간격으로 화면에 시간 출력하기
	@Override 
	public void run() {
			Date d = new Date();//시간 하는거 가장 간단한게 date 객체 만드는 거
	}
}
